angular.module('jakubniewczas')
  .directive('listSection', function() {
    return {
      scope: {
        title: '@',
        items: '@'
      },
      templateUrl: '<%= ng_template_url("list-section/") %>',
      controller: ['$scope', function($scope) {
        this.setItems = function(items) {
          $scope.items = items;
        }
      }]
    }
  })